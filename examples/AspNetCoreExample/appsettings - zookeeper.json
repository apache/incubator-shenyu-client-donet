{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Shenyu": {
    "Register": {
      "ServerList": "127.0.0.1:2181",
      "RegisterType": "zookeeper",
      "Props": {
        "Password": "",
        "SessionTimeout": 600000,
        "ConnectionTimeout": 600000,
        "BaseSleepTime": 15000,
        "MaxSleepTime": 60000,
        "MaxRetries": 3
      }
    },
    "Client": {
      "AppName": "dotnet-example-zookeeper",
      "ContextPath": "/zkdotnet",
      "IsFull": false,
      "ClientType": "http"
    }
  }
}
