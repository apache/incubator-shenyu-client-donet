{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Shenyu": {
    "Register": {
      "ServerList": "http://127.0.0.1:2379",
      "RegisterType": "etcd",
      "Props": {
        "UserName": "",
        "Password": "",
        "EtcdTimeout": 4000,
        "EtcdTTL": 15
      }
    },
    "Client": {
      "AppName": "dotnetexampleetcd",
      "ContextPath": "/etcddotnet",
      "IsFull": false,
      "ClientType": "http"
    }
  }
}
